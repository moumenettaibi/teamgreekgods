{
    "version": 2,
    "builds": [
        { "src": "main.py", "use": "@vercel/python" }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/main.py" }
    ]
}
